{
    "tslint.enable": true,
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "trailing-comma":  [ false ],
      "member-ordering": [ false ],
      "max-line-length": [ true, 180 ],
      "no-console":      [ false ],
      "ordered-imports": [ false ],
      "no-var-requires": false,
      "object-literal-sort-keys": [ false ],
      "arrow-parens": false,
      "comment-format":  [
        true,
        { "ignore-words": [ "TODO" ] }
      ],
      "typedef": [
        true,
        "call-signature",
        "property-declaration"
      ]
    },
    "rulesDirectory": []
  }